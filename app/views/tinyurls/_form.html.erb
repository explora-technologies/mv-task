<div class="row card card-panel " style="padding-top:10%">
<%= form_tag("tinyurl", {:method => :post, :remote => false,:id=>"tnyurl",:class=>"col s12" }) do %>
	<div class="row">
		<div class="input-field col s12">
		    <%= text_field_tag :url %>
		    <label for="url">URL </label>
		</div>
		<center><%= submit_tag 'Create a tiny url' %></center>
	</div>
<% end %>
</div>